<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fb="http://schemas.android.com/apk/res-auto">
    <LinearLayout android:orientation="vertical" android:id="@id/business_create_account_content_container" android:visibility="invisible" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <ScrollView android:layout_width="fill_parent" android:layout_height="0.0dip" android:layout_weight="1.0">
            <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/native_sign_up_layout_margin_horizontal_default">
                <ImageView android:id="@id/business_sign_up_back_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/native_sign_up_cross_margin_top" android:src="@drawable/msgr_ic_arrow_back" android:tint="@color/black" android:alpha="@dimen/native_sign_up_default_alpha" />
                <include layout="@layout/native_sign_up_pair_profile_view" />
                <com.facebook.widget.text.BetterTextView android:id="@id/business_create_account_header_message" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/native_sign_up_message_text" />
                <com.facebook.messaging.business.nativesignup.view.NativeSignUpRowView android:id="@id/business_create_account_profile_row" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/native_sign_up_row_list_margin_top" fb:nativeSignUpRowIconDrawable="@drawable/msgr_ic_account_circle" />
                <com.facebook.messaging.business.nativesignup.view.NativeSignUpRowView android:id="@id/business_create_account_payment_row" android:layout_width="fill_parent" android:layout_height="wrap_content" fb:nativeSignUpRowIconDrawable="@drawable/msgr_ic_card" />
                <com.facebook.messaging.business.nativesignup.view.NativeSignUpRowView android:id="@id/business_create_account_email_row" android:layout_width="fill_parent" android:layout_height="wrap_content" fb:nativeSignUpRowIconDrawable="@drawable/msgr_ic_email" />
                <com.facebook.messaging.business.nativesignup.view.NativeSignUpRowView android:id="@id/business_create_account_phone_row" android:layout_width="fill_parent" android:layout_height="wrap_content" fb:nativeSignUpRowIconDrawable="@drawable/msgr_ic_call" />
            </LinearLayout>
        </ScrollView>
        <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/native_sign_up_terms_margin_bottom" style="@style/native_sign_up_layout_margin_horizontal_default">
            <CheckBox android:id="@id/business_create_account_checkbox" android:focusable="false" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" />
            <com.facebook.widget.text.BetterTextView android:textSize="@dimen/native_sign_up_terms_font_size" android:layout_gravity="center_vertical" android:id="@id/business_create_account_terms_text" android:layout_width="fill_parent" android:layout_height="wrap_content" />
        </LinearLayout>
        <com.facebook.widget.text.BetterButton android:enabled="false" android:id="@id/business_create_account_create_account_button" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/native_sign_up_margin_horizontal" android:layout_marginRight="@dimen/native_sign_up_margin_horizontal" android:layout_marginBottom="@dimen/native_sign_up_layout_bottom_margin" android:text="@string/native_sign_up_create_account" android:height="@dimen/native_sign_up_button_height" android:layout_marginStart="@dimen/native_sign_up_margin_horizontal" android:layout_marginEnd="@dimen/native_sign_up_margin_horizontal" style="@style/Payments.Button" />
    </LinearLayout>
    <ProgressBar android:layout_gravity="center" android:id="@id/business_create_account_progress_bar" android:layout_width="wrap_content" android:layout_height="wrap_content" style="?android:attr/progressBarStyle" />
</FrameLayout>