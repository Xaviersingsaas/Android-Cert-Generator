<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:height="@dimen/photo_capture_dim" android:width="@dimen/photo_capture_dim" />
            <solid android:color="@color/transparent" />
            <stroke android:width="@dimen/photo_capture_outerborder" android:color="@color/solid_white" />
        </shape>
    </item>
    <item android:left="@dimen/photo_capture_innerborder" android:top="@dimen/photo_capture_innerborder" android:right="@dimen/photo_capture_innerborder" android:bottom="@dimen/photo_capture_innerborder">
        <shape android:shape="oval">
            <solid android:color="@color/photo_capture_innercircle" />
        </shape>
    </item>
</layer-list>