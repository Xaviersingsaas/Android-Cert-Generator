<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/fbui_bluegrey_5" />
            <stroke android:width="1.0dip" android:color="@color/fbui_accent_blue" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fbui_white" />
            <stroke android:width="1.0dip" android:color="@color/fbui_accent_blue" />
        </shape>
    </item>
</selector>