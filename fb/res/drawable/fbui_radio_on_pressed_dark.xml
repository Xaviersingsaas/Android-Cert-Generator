<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/fbui_check_pressed_dark" />
            <size android:height="@dimen/fbui_check_size" android:width="@dimen/fbui_check_size" />
        </shape>
    </item>
    <item android:left="@dimen/fbui_check_outer_ring_inset" android:top="@dimen/fbui_check_outer_ring_inset" android:right="@dimen/fbui_check_outer_ring_inset" android:bottom="@dimen/fbui_check_outer_ring_inset">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:width="@dimen/fbui_check_outer_ring_stroke_size" android:color="@color/fbui_bluegrey_40" />
        </shape>
    </item>
    <item android:left="@dimen/fbui_check_inner_ring_inset" android:top="@dimen/fbui_check_inner_ring_inset" android:right="@dimen/fbui_check_inner_ring_inset" android:bottom="@dimen/fbui_check_inner_ring_inset">
        <shape android:shape="oval">
            <solid android:color="@color/fbui_bluegrey_10" />
        </shape>
    </item>
</layer-list>