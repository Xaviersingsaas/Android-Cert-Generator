<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fbui_white" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fbui_divider" />
        </shape>
    </item>
    <item android:top="1.0dip">
        <shape android:shape="rectangle">
            <solid android:color="@color/fbui_white" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="@color/fbui_pressed_list_item_bg" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>