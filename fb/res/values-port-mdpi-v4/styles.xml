        <item name="android:textSize">@dimen/fbui_text_size_small</item>
    </style>
    <style name="ContactsTermsToggleScrollViewTheme">
        <item name="android:fadeScrollbars">true</item>
    </style>
    <style name="ContactsTermsToggleTermTextTitle">
        <item name="android:textSize">@dimen/fbui_text_size_xlarge</item>
