    <style name="ContactsTermsOptOutTheme">
        <item name="android:layout_marginBottom">@dimen/registration_contacts_terms_medium_small_vertical_margin</item>
    </style>
    <style name="ContactsTermsTermTextTitle">
        <item name="android:textSize">@dimen/fbui_text_size_small</item>
    </style>
    <style name="ContactsTermsTermTextSubtitle">
        <item name="android:textSize">@dimen/fbui_text_size_small</item>
    </style>
