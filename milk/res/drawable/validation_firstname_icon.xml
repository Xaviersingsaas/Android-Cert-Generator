<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res/com.samsung.milk.milkvideo">
    <item app:state_failure="true">
        <bitmap android:src="@drawable/firstname_error" />
    </item>
    <item android:state_focused="true">
        <bitmap android:src="@drawable/firstname_active" />
    </item>
    <item android:state_focused="false">
        <bitmap android:src="@drawable/firstname_steady" />
    </item>
</selector>